{
  "expo": {
    "name": "My Notes App (TypeScript)",
    "slug": "my-notes-app-demo",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Simple note-taking app. Using typescript, hooks, and local state to store or modify notes.",
    "githubUrl": "https://github.com/mallok/myNotesApp.git"
  }
}
